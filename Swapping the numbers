//take the input from the user


#include <stdio.h>
void main()
{
    int i,j;
    printf("enter the i num:-");
    scanf("%d",&i);
    printf("enter the j num:-");
    scanf("%d",&j);
    printf("before swap/n i=%d,j=%d \n",i,j);
    i=i^j;     //here u can use any logic from the given logics
    j=i^j;
    i=i^j;
    printf("after swap /n i=%i,j=%d \n",i,j);
}



// first method

#include <stdio.h>
void main()
{
    int i=2,j=5,k;
    printf("i=%d,j=%d,\n",i,j);
    k=i;
    i=j;
    j=k;
    printf("i=%i,j=%d\n",i,j);
}


// sec method

#include <stdio.h>
void main()
{
    int i=2,j=5;
    printf("i=%d,j=%d,\n",i,j);
    i=i+j;
    j=i-j;
    i=i-j;
    printf("i=%i,j=%d\n",i,j);
}


//third method

#include <stdio.h>
void main()
{
    int i=2,j=5;
    printf("i=%d,j=%d,\n",i,j);
    i=i^j;
    j=i^j;
    i=i^j;
    printf("i=%i,j=%d\n",i,j);
}


