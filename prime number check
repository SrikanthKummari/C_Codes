#include <stdio.h>
int prime(void){
    int num;
    printf("enter the positive num\n");
    scanf("%d",&num);
    return num;
    
}

int main() {
    int a,b,c=0;
    a=prime();
    for(b=1,c=0;b<=a;b++)
        if(a%b==0)
        c++;
        
    if(c==2)
    printf("%d is prime num\n",a);
    else
    printf("%d is not a prime\n",a);
    return 0;
}
